{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/dist/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/work.html",
      "dest": "/work.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
